<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p" 
 	xsi:schemaLocation="http://www.springframework.org/schema/beans
 	http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="accountbean" class="com.durgasoft.beans.Account" p:accno="101" p:accname="vihaan" p:balance="1000"/>

<bean id="employeebean" class="com.durgasoft.beans.Employee" p:empid="1234" p:empname="VihaanReddy" p:empdesg="manager" p:acc-ref="accountbean"/>

</beans>